node "Other Polkadot Ecosystem" as OPE {
  [Other Marketplaces]
}

package "Protocol Layer/Hypha Spores" {
  [Fruniques Pallet]
  [Marketplace Pallet] --> [Other Marketplaces]
  [Advanced Frunique UI/UX]
  [Services and Caching]
